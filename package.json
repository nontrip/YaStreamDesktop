{
  "name": "YaStream",
  "version": "1.1.1",
  "main": "main.js",
  "license": "MIT",
  "repository": "b52/electron-es6-react",
  "scripts": {
    "start": "electron -r babel-register .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dmg": "electron-builder -mwl" 
  },
  "build": {
    "appId": "yastream.win",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "files": [
      "!goal.txt",
      "!goalToOpen.txt",
      "!log.txt"
    ],
    "extraFiles": [
      "goal.txt",
      "goalToOpen.txt",
      "log.txt"
    ]
  },

  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "electron": "^1.4.3",
    "electron-builder": "^19.19.1"
  }
}